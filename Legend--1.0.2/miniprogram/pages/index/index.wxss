/**index.wxss**/
@import '../../common/common.wxss';
.swiperClass {
  margin: 0;
  margin-top: 10px;
  margin-bottom: 10px;

}

.slide-image {
  width: 100%; 
  height: 100%;
  border-radius: 10px 10px;
  position: relative;
}

image.active {
  transform: none;
  transition: all 1s ease-in 0s;
} 

image.quiet {
  transform: scale(0.9);
  transition: all 1s ease-in 0s;
}
